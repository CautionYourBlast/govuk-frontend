<div class="ds_skip-links">
  <ul class="ds_skip-links__list">
    <li class="ds_skip-links__item">
      <a href="{{ params.href | default("#content", true) }}" class="ds_skip-links__link{%- if params.classes %} {{ params.classes }}{% endif -%}"{%- for attribute, value in params.attributes %} {{ attribute }}="{{ value }}"{% endfor %} data-module="scot-gov-skip-link">
        {{- params.html | safe if params.html else params.text -}}
      </a>
    </li>
  </ul>
</div>